{
  "name": "boilerplate-svelte",
  "version": "1.0.0",
  "repository": "github:lagden/boilerplate-svelte",
  "description": "Boilerplate para desenvolvimento de uma aplicação utilizando Svelte.",
  "keywords": [
    "svelte",
    "template",
    "boilerplate"
  ],
  "type": "module",
  "main": "public/index.html",
  "files": [
    "public",
    "README.md"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "scripts": {
    "rm": "rm -rf public/*",
    "cp": "cp -R static/* public/.",
    "sprite": "spritetify -d ./src/_assets/svg/ -o ./src/_components/_global/Sprite.svelte -c",
    "eslint": "eslint --config .eslintrc.cjs --ext .js,.svelte --ignore-path .gitignore .",
    "prettier": "prettier -c --plugin-search-dir=. ./src/**/*.{svelte,js}",
    "format": "prettier --write --plugin-search-dir=. ./src/**/*.{svelte,js}",
    "test": "run-s eslint",
    "esbuild": "node ./esbuild.config.js",
    "build": "run-s test rm cp esbuild",
    "prestart": "npm test",
    "start": "vite --cors --port ${PORT:-5000} --host ${HOSTNAME_CUSTOM:-0.0.0.0}"
  },
  "dependencies": {
    "sirv-cli": "2.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@babel/eslint-parser": "7.18.2",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.49",
    "@tadashi/common": "1.4.1",
    "@tadashi/spritetify": "2.1.0",
    "@tadashi/svelte-icon": "3.0.0",
    "autoprefixer": "10.4.7",
    "esbuild": "0.14.47",
    "esbuild-svelte": "0.7.1",
    "eslint": "8.18.0",
    "eslint-config-xo": "0.41.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-svelte3": "4.0.0",
    "eslint-plugin-unicorn": "42.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "postcss-import": "14.1.0",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "svelte": "3.48.0",
    "svelte-preprocess": "4.10.7",
    "tailwindcss": "3.1.4",
    "vite": "2.9.12"
  },
  "babel": {},
  "packageManager": "yarn@3.2.1"
}
